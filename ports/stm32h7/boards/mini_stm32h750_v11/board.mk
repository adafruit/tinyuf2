SPI_FLASH = W25Qx_SPI
QSPI_FLASH = W25Qx_QSPI
DISPLAY_DRV = ST7735
JLINK_DEVICE = stm32h750vb

CFLAGS += \
  -DSTM32H750xx \
  -DHSE_VALUE=25000000U \
  -DTINYUF2_DISPLAY=1 \
  -DTINYUF2_LED=1 \
  -DBOARD_QSPI_FLASH_EN=1 \
  -DBOARD_SPI_FLASH_EN=1 \
  -DBOARD_AXISRAM_EN=1 \
  -D$(SPI_FLASH) \
  -D$(QSPI_FLASH)\
  -DBOARD_FLASH_APP_START=0x90000000 \
  -DTRAP_EXC


MCU = h750xx

SRC_S += \
  $(ST_CMSIS)/Source/Templates/gcc/startup_stm32h750xx.s
